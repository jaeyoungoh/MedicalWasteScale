<?xml version="1.0" encoding="utf-8"?>
<shape android:shape="rectangle"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <gradient
        android:type="linear"
        android:startColor="#fee6d5"
        android:centerColor="#f4ece2"
        android:endColor="#dad5d2"
        android:angle="270"/>
    <stroke
        android:width="3dp"
        android:color="#fee6d5"/>
    <corners
        android:radius="80dp"/>
    <padding
        android:top="2dp"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"/>
</shape>